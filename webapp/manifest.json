{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ansaldowbselement",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"sourceTemplate": {
			"id": "generator-ui5-app",
			"version": "1.1.0"
		},
		"dataSources": {
      "ZMAN_PID046_EBOM_SRV": {
        "uri": "/sap/opu/odata/sap/zman_pid046_ebom_srv",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
			"ZLOG_PID999_INTEGRATION_SRV": {
        "uri": "/sap/opu/odata/sap/ZLOG_PID999_INTEGRATION_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "ansaldowbselement.view.App",
			"type": "XML",
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.141.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ansaldowbselement.i18n.i18n",
					"supportedLocales": ["en", "de"],
					"fallbackLocale": "en",
					"async": true
				}
			},
			"ZMAN_PID046_EBOM_SRV": {
        "dataSource": "ZMAN_PID046_EBOM_SRV",
        "preload": true,
        "settings": {
          "defaultOperationMode": "Server",
          "useBatch": true,
          "refreshAfterChange": true
        }
      },
				"ZLOG_PID999_INTEGRATION_SRV": {
        "dataSource": "ZLOG_PID999_INTEGRATION_SRV",
        "preload": true,
        "settings": {
          "defaultOperationMode": "Server",
          "useBatch": true,
          "refreshAfterChange": true
        }
      }
		},
		"resources": {
			"css": [
			  {
				"uri": "css/style.css"
			  }
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ansaldowbselement.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				}
			}
		}
	}
}
